<div class="p-3">
 <h2 class="my-3"> Productos Test</h2>
   <table class="table mb-3">
      <thead>
         <tr>
         <th scope="col">Title</th>
         <th scope="col">Price</th>
         <th scope="col">Thumbnail</th>  
      </tr>
   </thead>
   <tbody>

      {{#each products}}
      <tr>            
         <td>{{this.title}}</td>
         <td>{{this.price}}</td>
         <td><img src="{{this.thumbnail}}" /></td>
      </tr>      
      {{/each }}

   </tbody>
   </table>

</div>

